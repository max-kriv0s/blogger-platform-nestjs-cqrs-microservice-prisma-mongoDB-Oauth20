{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleDirectories": ["<rootDir>/../", "node_modules"],
  "moduleNameMapper": {
    "^apps/inctagram-gateway/src/(.*)$": "<rootDir>/$1"},
  "testTimeout": 15000
}
